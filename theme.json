{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": false,
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"color": {
			"background": true,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"link": true,
			"palette": [
				{
					"color": "#333F2E",
					"name": "Green",
					"slug": "green"
				},
				{
					"color": "#3F362E",
					"name": "Brown",
					"slug": "brown"
				},
				{
					"color": "#454545",
					"name": "Grey",
					"slug": "grey"
				},
				{
					"color": "#f8f6f4",
					"name": "Grey light",
					"slug": "grey-light"
				},
				{
					"color": "#fff",
					"name": "White",
					"slug": "white"
				}
			],
			"text": true
		},
		"custom": {},
		"dimensions": {
			"aspectRatio": false,
			"aspectRatios": [],
			"defaultAspectRatios": false,
			"minHeight": false
		},
		"layout": {
			"allowCustomContentAndWideSize": false,
			"allowEditing": true,
			"contentSize": "770px",
			"wideSize": "1210px"
		},
		"position": {
			"sticky": false
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Shadow btn dark",
					"slug": "natural",
					"shadow": "0 0 25px rgba(0, 0, 0, 0.2)"
				},
				{
					"name": "Shadow product box",
					"slug": "deep",
					"shadow": "0 0 20px rgba(0, 0, 0, 0.2)"
				},
				{
					"name": "Shadow blog box",
					"slug": "sharp",
					"shadow": "0 0 25px rgba(0, 0, 0, 0.5)"
				},
				{
					"name": "Shadow menu",
					"slug": "outlined",
					"shadow": "0 10px 30px rgba(0, 0, 0, 0.3)"
				}
			]
		},
		"spacing": {
			"blockGap": null,
			"customSpacingSize": false,
			"defaultSpacingSizes": false,
			"margin": false,
			"padding": false,
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "5",
					"size": "5px",
					"slug": "5"
				},
				{
					"name": "10",
					"size": "10px",
					"slug": "10"
				},
				{
					"name": "15",
					"size": "15px",
					"slug": "15"
				},
				{
					"name": "20",
					"size": "20px",
					"slug": "20"
				},
				{
					"name": "30",
					"size": "30px",
					"slug": "30"
				},
				{
					"name": "40",
					"size": "40px",
					"slug": "40"
				},
				{
					"name": "50",
					"size": "50px",
					"slug": "50"
				},
				{
					"name": "100",
					"size": "100px",
					"slug": "100"
				}
			]
		},
		"typography": {
			"fluid": {
				"maxViewportWidth": "768px",
				"minViewportWidth": "375px"
			},
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": 500,
							"src": ["file:./fonts/montserrat-medium.woff2"]
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": 600,
							"src": ["file:./fonts/montserrat-semibold.woff2"]
						},
						{
							"fontFamily": "Monsterrat",
							"fontStyle": "normal",
							"fontWeight": 700,
							"src": ["file:./fonts/montserrat-bold.woff2"]
						}
					],
					"fontFamily": "Montserrat, -apple-system, BlinkMacSystemFont, Segoe UI, Roboto, Helvetica Neue, Arial, sans-serif",
					"name": "Montserrat",
					"slug": "body"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Playfair Display",
							"fontStyle": "normal",
							"fontWeight": 500,
							"src": ["file:./fonts/playfair_display-medium.woff2"]
						}
					],
					"fontFamily": "Playfair Display, Iowan Old Style, Apple Garamond, Baskerville, Times New Roman, Droid Serif, Times, Source Serif Pro, serif",
					"name": "Playfair Display",
					"slug": "headings"
				}
			],
			"customFontSize": false,
			"defaultFontSizes": false,
			"dropCap": false,
			"fontSizes": [
				{
					"name": "Huge",
					"slug": "huge",
					"size": "90px",
					"fluid": {
						"max": "90px",
						"min": "45px"
					}
				},
				{
					"name": "Extra Large",
					"slug": "x-large",
					"size": "50px",
					"fluid": {
						"max": "50px",
						"min": "30px"
					}
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "30px",
					"fluid": false
				},
				{
					"name": "Big",
					"slug": "big",
					"size": "24px",
					"fluid": false
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "20px",
					"fluid": false
				},
				{
					"name": "Medium S",
					"slug": "medium-sm",
					"size": "18px",
					"fluid": {
						"max": "18px",
						"min": "16px"
					}
				},
				{
					"name": "Normal",
					"slug": "normal",
					"size": "16px",
					"fluid": {
						"max": "16px",
						"min": "15px"
					}
				},
				{
					"name": "Small",
					"slug": "small",
					"size": "14px",
					"fluid": false
				},
				{
					"name": "Extra Small",
					"slug": "x-small",
					"size": "12px",
					"fluid": false
				}
			],
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false,
			"textAlign": true,
			"textColumns": false,
			"textDecoration": true,
			"textTransform": true,
			"writingMode": false
		},
		"useRootPaddingAwareAlignments": true,
		"blocks": {
			"core/button": {
				"border": {
					"radius": false
				}
			},
			"core/pullquote": {
				"border": {
					"color": false,
					"radius": false,
					"style": false,
					"width": false
				}
			},
			"core/group": {
				"spacing": {
					"padding": true,
					"margin": true
				},
				"background": {
					"backgroundImage": true,
					"backgroundSize": true
				},
				"layout": {
					"contentSize": "570px",
					"wideSize": "1170px"
				}
			}
		}
	},
	"styles": {
		"color": {
			"text": "var:preset|color|grey"
		},
		"spacing": {
			"padding": {
				"left": "var:preset|spacing|20",
				"right": "var:preset|spacing|20"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|body",
			"fontSize": "var:preset|font-size|normal",
			"fontWeight": "500",
			"lineHeight": "1.5"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|headings",
					"fontWeight": "500",
					"letterSpacing": "0.05em"
				},
				"color": {
					"text": "var:preset|color|brown"
				},
				"spacing": {
					"margin": {
						"top": "1em",
						"bottom": "0.5em"
					}
				},
				"css": "&:first-child { margin-top: 0 }"
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|x-large",
					"lineHeight": "1.15"
				},
				"spacing": {
					"margin": {
						"top": "0.35em",
						"bottom": "0.35em"
					}
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|x-large",
					"lineHeight": "1.15"
				},
				"spacing": {
					"margin": {
						"top": "0.75em",
						"bottom": "0.5em"
					}
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|large",
					"lineHeight": "1.15"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|big"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var:preset|font-family|body",
					"fontWeight": "700",
					"letterSpacing": "0.2em",
					"textTransform": "uppercase",
					"fontSize": "var:preset|font-size|x-small"
				},
				"spacing": {
					"margin": {
						"top": "2em",
						"bottom": "1em"
					}
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var:preset|font-family|body",
					"fontWeight": "700",
					"letterSpacing": "normal",
					"fontSize": "var:preset|font-size|normal"
				},
				"color": {
					"text": "var:preset|color|grey"
				},
				"spacing": {
					"margin": {
						"top": "1.3em",
						"bottom": "0.625em"
					}
				}
			},
			"caption": {
				"typography": {
					"letterSpacing": "0.2em",
					"fontSize": "var:preset|font-size|small"
				},
				"color": {
					"text": "var(--color--grey-2)"
				}
			},
			"link": {
				"typography": {
					"fontWeight": "700",
					"textDecoration": "underline"
				},
				"color": {
					"text": "var:preset|color|green"
				},
				"css": "transition: color 0.35s ease-out"
			}
		},
		"blocks": {
			"core/paragraph": {
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "0.625em"
					}
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "underline"
						},
						":hover": {
							"color": {
								"text": "var(--color--red-1)"
							}
						}
					}
				},
				"css": "&:last-child { margin-bottom: 0; }"
			},
			"core/list": {
				"spacing": {
					"margin": {
						"top": "0",
						"right": "0",
						"bottom": "0.625em",
						"left": "0"
					},
					"padding": {
						"left": "1.5em"
					}
				},
				"css": "&:is(ul) { list-style-type: \"\\2014\"; } & :where(ul, ol) { margin-bottom: 0 }"
			},
			"core/list-item": {
				"spacing": {
					"padding": {
						"left": "min(1vw, 1em)"
					},
					"margin": {
						"bottom": "0.5em"
					}
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "underline"
						},
						":hover": {
							"color": {
								"text": "var(--color--red-1)"
							}
						}
					}
				},
				"css": "& :where(ul, ol) { margin-top: 0.5em }"
			},
			"core/image": {
				"spacing": {
					"margin": {
						"top": "1.5em",
						"bottom": "1.5em"
					}
				},
				"border": {
					"radius": "25px"
				},
				"typography": {
					"textAlign": "center"
				},
				"elements": {
					"caption": {
						"typography": {
							"textAlign": "left"
						}
					}
				}
			},
			"core/embed": {
				"spacing": {
					"margin": {
						"top": "1.5em",
						"bottom": "1.5em"
					}
				},
				"typography": {
					"textAlign": "center"
				},
				"elements": {
					"caption": {
						"typography": {
							"textAlign": "left"
						}
					}
				}
			},
			"core/footnotes": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"color": {
								"text": "var(--color--red-1)"
							}
						}
					}
				}
			},
			"core/details": {
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "1.25em"
					}
				}
			}
		}
	},
	"customTemplates": [],
	"templateParts": [],
	"patterns": []
}
